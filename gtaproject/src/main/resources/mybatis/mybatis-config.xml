<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- Oracle 테이블의 컬럼이 mem_id 이고, DTO의 필드변수가 memId 일때 매칭이 가능하게 하는 옵션 -->
		<setting name="mapUnderscoreToCamelCase" value="true" />
		<!-- 널허용 -->
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>



	<!-- mapper vo 매핑 -->
	<typeAliases>
		<typeAlias type="com.spring.gta.inform.vo.InformVO" alias="InformVO" />
		<typeAlias type="com.spring.gta.login.vo.LoginVO" alias="LoginVO" />
		<typeAlias type="com.spring.gta.dashboard.vo.DashboardVO" alias="DashboardVO" />
		<typeAlias type="com.spring.gta.inform.vo.SearchVO" alias="SearchVO" />
		<typeAlias type="com.spring.gta.dashboard.vo.DashboardsearchVO" alias="DashboardsearchVO" />
		<typeAlias type="com.spring.gta.dashboard.vo.ChartVO" alias="ChartVO" />
	</typeAliases>
</configuration>